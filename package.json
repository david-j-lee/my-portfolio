{
  "name": "my-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "preact build",
    "build-no-prerender": "preact build --no-prerender",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest",
    "publish": "npm run build-no-prerender && aws s3 sync build/ s3://devdavidlee.com"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.0.1",
    "eslint": "^8.20.0",
    "eslint-config-preact": "^1.3.0",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.5",
    "node-sass": "^7.0.1",
    "preact-cli": "^3.4.0",
    "sass-loader": "^10.3.1",
    "sirv-cli": "2.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "animate.css": "^4.1.1",
    "animejs": "^3.2.1",
    "bootstrap": "^4.0.0",
    "particles.js": "^2.0.0",
    "preact": "^10.10.0",
    "preact-render-to-string": "^5.2.1",
    "preact-router": "^4.1.0"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
